{
	"name": "@webstrake/beachflow-svelte",
	"version": "1.1.6",
	"scripts": {
		"dev": "vite dev",
		"build:vite": "vite build",
		"build": "npm run local && npm run git",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"local": "npm run clean && npm run package && npm run copy && npm run postbuild",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"copy": "node scripts/copy-common-files.js",
		"clean": "rmdir /s /q dist",
		"postbuild": "node scripts/post-build.js",
		"git": "cd /d D:\\webStrake-main\\webstrake-beachflow-svelte && git add . && git commit -m \"new build\" && git push"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.2.7"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"@types/luxon": "^3.4.2",
		"@webstrake/beachflow": "github:webstrake/webstrake-beachflow",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.49",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.16",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"luxon": "^3.5.0"
	}
}
